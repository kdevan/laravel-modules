{
  "name": "kdevan/laravel-modules",
  "description": "Laravel Module management",
  "version": "dev-master",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com",
      "homepage": "https://nicolaswidart.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^3.6",
    "friendsofphp/php-cs-fixer": "^2.7",
    "laravel/framework": "5.6.*",
    "spatie/phpunit-snapshot-assertions": "^1.0",
    "phpstan/phpstan": "^0.9.2"
  },
  "autoload": {
    "psr-4": {
      "Kdevan\\Modules\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kdevan\\Modules\\Tests\\": "tests",
      "Modules\\Recipe\\": "tests/stubs/valid/Recipe"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kdevan\\Modules\\LaravelModulesServiceProvider"
      ],
      "aliases": {
        "Module": "Kdevan\\Modules\\Facades\\Module"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
